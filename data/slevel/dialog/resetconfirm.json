{
  "type": "minecraft:multi_action",
  "title": {
    "text": "[Reset levels]",
    "type": "text",
    "color": "light_purple"
  },
  "body": [
    {
      "type": "minecraft:plain_message",
      "contents": "Do you want to reset your scale levels? Your decision is final after pressing \"Confirm\"."
    }
  ],
  "inputs": [],
