  "pause": false,
  "actions": [
    {
      "label": {
        "text": "Confirm"
      },
      "action": {
        "type": "minecraft:run_command",
        "command": "/trigger ResetFinal"
      }
    },
    {
      "label": "Go back",
      "action": {
        "type": "minecraft:show_dialog",
        "dialog": "slevel:helpmenu"
      }
    }
  ]
}